<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="5.0" ExtensionBundleId="com.workflowmediapipe.mediasolution" ExtensionBundleVersion="12.0.0"
		ExtensionBundleName="MediaSolution" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.workflowmediapipe.mediasolution" Version="12.0.0" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
            <Host Name="AEFT" Version="[13.0,99.9]" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="5.0" />
		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.workflowmediapipe.mediasolution">
			<DispatchInfo >
				<Resources>
				<MainPath>./client/index.html</MainPath>
				<ScriptPath>./host/MediaSolution.jsx</ScriptPath>
                <CEFCommandLine>
                    <Parameter>--enable-nodejs</Parameter>
                    <Parameter>--mixed-context</Parameter>
                </CEFCommandLine>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>Media Solution v12.0</Menu>
					<Geometry>
						<Size>
							<Height>800</Height>
							<Width>600</Width>
						</Size>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>
