{
  "output": {
    "style": "markdown",
    "filePath": "repomix-after-effects-output.md",
    "removeComments": false,
    "showLineNumbers": true,
    "topFilesLength": 100,
    "copyWithoutSorting": true
  },
  "include": [
    "docs/**/*.md",
    ".windsurf/rules/codingstandards.md",
    ".windsurf/skills/**/*"
  ],
  "ignore": {
    "customPatterns": [
      "Scripts_AE/**",
      "docs/internal/repomix/**",
      "repomix.md",
      "repomix-after-effects-output.md",
      ".idea/**",
      "PyShiftAE/**",
      "PyShiftBridge/**",
      "**/*.mp4",
      "**/*.mov",
      "**/*.mp3",
      "**/*.wav",
      "**/*.aep",
      "**/*.ffx",
      "**/*.zip",
      "**/*.rar",
      "**/*.7z",
      "**/*.exe",
      "**/*.dmg",
      "**/*.pkg",
      "**/*.pdf",
      "**/*.png",
      "**/*.jpg",
      "**/*.jpeg",
      "**/*.gif",
      "**/*.svg",
      "**/*.ico",
      "**/*.psd",
      "**/*.ai",
      "**/*.reg",
      "**/*.url",
      "Aescripts-Social Importer 1.0.3 for After Effects/Social Importer/panel.js"
    ]
  }
}
