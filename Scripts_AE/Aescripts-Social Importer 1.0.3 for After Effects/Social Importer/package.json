{
  "name": "social-importer",
  "version": "1.0.3",
  "main": "index.js",
  "license": "MIT",
  "description": "An After Effects plugin for importing and rendering social content automatically.",
  "extendScriptClass": "SocialImporter",
  "manifest": {
    "bundleName": "Social Importer",
    "bundleId": "com.aescripts.socialimporter",
    "hosts": {
      "AEFT": "13.0"
    },
    "requiredRuntimes": {
      "CSXS": "5.0"
    },
    "extensions": {
      "panel": {
        "title": "Social Importer",
        "size": [
          300,
          300
        ],
        "minSize": [
          210,
          200
        ]
      },
      "about": {
        "title": "About Social Importer",
        "type": "Modeless",
        "size": [
          350,
          440
        ],
        "maxSize": [
          350,
          500
        ],
        "minSize": [
          350,
          100
        ],
        "manifestOnly": true,
        "showMenu": false,
        "mainPath": "./dialog/dialog.html",
        "scriptPath": null
      }
    }
  },
  "private": true,
  "scripts": {
    "build": "neutrino build",
    "start": "neutrino start",
    "lint": "neutrino lint",
    "test": "neutrino test",
    "release": "standard-version -r",
    "deploy-local": "cep-scripts deploy-local",
    "package": "cep-scripts package",
    "publish-release": "cep-scripts release"
  },
  "devDependencies": {
    "@cep/csinterface": "^8.0.1",
    "@neutrinojs/jest": "^8",
    "@neutrinojs/standardjs": "^8",
    "@pluginplay/cep-scripts": "^0.0.4",
    "@sentry/webpack-plugin": "1.5.2",
    "axios": "^0.19.0",
    "buffer": "^5.2.1",
    "cep-lib": "^0.1.0",
    "chai": "^4",
    "chai-jest-diff": "^1",
    "dirty-chai": "^2",
    "instagram-nodejs-without-api": "^1.2.0",
    "jest": "^23",
    "jest-junit": "^5",
    "jest-styled-components": "^6",
    "lodash": "^4.17.11",
    "mobx": "^4",
    "mobx-react": "^5",
    "moment": "^2.24.0",
    "neutrino": "^8",
    "neutrino-preset-cep": "^0.0.17",
    "numbro": "^2.1.2",
    "prop-types": "^15",
    "raven-js": "^3.27.0",
    "react": "^16",
    "react-cep": "^0.1.0",
    "react-dom": "^16",
    "react-hot-loader": "^3",
    "standard-version": "^4",
    "styled-components": "^3",
    "styled-flex-component": "^2",
    "twit": "^2.2.11",
    "uuid": "^3.3.2"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "temp": "^0.8.3",
    "ajv": "^6.10.1"
  }
}
